def binary_search(arr, target):
    lb = 0
    ub = len(arr) - 1
    while lb <= ub:
        mid = (lb + ub) // 2
        if arr[mid] == target:
            print("Element found at index", mid)
            return
        elif arr[mid] < target:
            lb = mid + 1
        else:
            ub = mid - 1
    print("Element not found in array")
arr=[10,20,30,40]
target=int(input("enter your target value"))
binary_search(arr,target)

